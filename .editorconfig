root = true

[*]
charset = utf-8

end_of_line = lf

insert_final_newline = true
trim_trailing_whitespace = true

[*.java]

indent_style = space
indent_size = 4

tab_width = 4

ij_java_keep_blank_lines_in_declarations = 1
ij_java_keep_blank_lines_in_code = 1
ij_java_keep_blank_lines_before_right_brace = 0